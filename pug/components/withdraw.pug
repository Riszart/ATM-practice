div.withdraw-container
  div.withdraw-content
    h2 retiro
    div.withdraw-content__data
      p nombre del propietario:
        span.name-ower.name-count
    div.withdraw-content__detail 
      p saldo disponible:
      p $
        span.money-count 
    div.withdraw-count 
      div.withdraw-count__one
        label(for="") cuenta:
          span.withdraw-number-count__one
        input.withdraw-count__one-input(type="checkbox" name="count")
      div.withdraw-count__two
        label(for="") cuenta:
          span.withdraw-number-count__two
        input.withdraw-count__two-input(type="checkbox" name="count")
    div.withdraw__detail
      p 
    div.withdraw-input-send
      div 
        label(for="input-withdraw") introduce el monto que deseas retirar:
        input.input__withdraw(type="number" name="input-withdraw")
      div.botton-content__withdraw 
        div.botton-cancel__withdraw
          p cancelar
        div.botton-send__withdraw
          p retirar